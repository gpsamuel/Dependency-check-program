PROCESS BEFORE OUTPUT.

MODULE %_INIT_PBO_J.

MODULE %_PBO_REPORT.

MODULE %_PF_STATUS.

MODULE %_S_TRKORR.

MODULE %_S_USER.

MODULE %_END_OF_PBO.

PROCESS AFTER INPUT.

  MODULE %_INIT_PAI_J.

CHAIN.
  FIELD  S_TRKORR-LOW.
  FIELD  S_TRKORR-HIGH.
  MODULE %_S_TRKORR.
ENDCHAIN.

CHAIN.
  FIELD  S_USER-LOW.
  FIELD  S_USER-HIGH.
  MODULE %_S_USER.
ENDCHAIN.

FIELD !P_OUT MODULE %_P_OUT .

FIELD !P_ALV MODULE %_P_ALV .

FIELD !P_STA MODULE %_P_STA .


CHAIN.
  FIELD  S_TRKORR-LOW.
  FIELD  S_TRKORR-HIGH.
  FIELD  S_USER-LOW.
  FIELD  S_USER-HIGH.
  FIELD P_OUT .
  FIELD P_ALV .
  FIELD P_STA .
    MODULE %_BLOCK_1001001.
ENDCHAIN.

CHAIN.
  FIELD  S_TRKORR-LOW.
  FIELD  S_TRKORR-HIGH.
  FIELD  S_USER-LOW.
  FIELD  S_USER-HIGH.
  FIELD P_OUT .
  FIELD P_ALV .
  FIELD P_STA .
  MODULE %_END_OF_SCREEN.
  MODULE %_OK_CODE_1000.
ENDCHAIN.
